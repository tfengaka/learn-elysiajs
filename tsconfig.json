{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2022",
    "moduleResolution": "node",
    "types": ["bun-types"],
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "~libs/*":["./src/libs/*"],
      "~modules/*":["./src/modules/*"],
      "~utils/*":["./src/utils/*"],
      "~middlewares/*":["./src/middlewares/*"]
    },
  }
}
